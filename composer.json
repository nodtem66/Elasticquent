{
  "name": "nodtem66/elasticquent",
  "type": "library",
  "description": "Maps Laravel Eloquent models to Elasticsearch types with cviebrock/laravel-elasticsearch",
  "keywords": [
    "elasticsearch",
    "eloquent",
    "laravel"
  ],
  "homepage": "https://github.com/nodtem66/Elasticquent",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Fairholm",
      "email": "adam.fairholm@gmail.com",
      "homepage": "http://adamfairholm.com"
    },
    {
      "name": "Jirawat I.",
      "email": "nodtem66@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/database": "~4.2|^5",
    "illuminate/config": "~4.2|^5",
    "nesbot/carbon": "~1.0",
    "cviebrock/laravel-elasticsearch": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2|~5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Elasticquent\\": "src/"
    }
  },
  "extra": {
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
